 long long maxSubarraySum(int arr[], int n){
        
        
        long long cur=0;
        long long max=-10000;
     for(int i=0;i<n;i++)
     {
        cur=cur+arr[i];
        if(cur>max)
        {
        max=cur;
        }
        if(cur<0)
        {
        cur=0;
        }
         
     }
     return max;
        
    }
